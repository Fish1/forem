<%- headers = ["Name", "Username", "Email address", "Status", "Joining date", "Last activity", "Organizations"] -%>
<%= CSV.generate_line headers -%>
<%- @users.each do |user| -%>
  <%- row = [user.name, user.username, user.email, user_status(user), local_date(user.registered_at), local_date(user.last_activity), user.organizations.pluck(:name)] -%>
  <%= CSV.generate_line(row).html_safe -%>
<%- end -%>